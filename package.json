{
  "name": "crawler",
  "version": "1.0.0",
  "private": true,
  "description": "The goal of the crawl script if to fetch as many valid email addresses from crawling web pages sources resulted from crawling search engines sources, according to specific and random search keys.",
  "keywords": [
    "links",
    "emails",
    "email addresses"
  ],
  "license": "UNLICENSED",
  "author": "Or Assayag <orassayag@gmail.com>",
  "contributors": [
    {
      "name": "Or Assayag",
      "email": "orassayag@gmail.com",
      "url": "https://github.com/orassayag"
    }
  ],
  "main": "src/monitor/monitor.js",
  "files": [
    "src",
    ".eslintignore",
    ".eslintrc",
    ".gitignore",
    ".jsbeautifyrc",
    ".npmignore",
    "package-lock.json",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "backup": "node src/scripts/backup.script.js",
    "start": "node src/monitor/monitor.js",
    "domains": "node src/scripts/domains.script.js",
    "delay": "node src/scripts/delay.script.js",
    "pre_load": "node src/scripts/preload.script.js",
    "cases": "node src/tests/emailAddressTestCases.test.js",
    "generator": "node src/tests/emailAddressGenerator.test.js",
    "val": "node src/tests/validateEmailAddress.test.js",
    "valmany": "node src/tests/validateEmailAddresses.test.js",
    "valdebug": "node --inspect-brk src/tests/validateEmailAddress.test.js",
    "typos": "node src/tests/emailAddressTypos.test.js",
    "sand": "node src/tests/sandBox.test.js",
    "link": "node src/tests/linkCrawl.test.js",
    "session": "node src/tests/linksSession.test.js",
    "preload": "npm run pre_load && npm i & npm run delay & npm i",
    "stop": "taskkill -f -im node.exe"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "ansi-escapes": "^4.3.1",
    "babel-eslint": "^10.1.0",
    "chance": "^1.1.7",
    "cli-cursor": "^3.1.0",
    "eslint": "^7.19.0",
    "faker": "^5.3.1",
    "forever-monitor": "^3.0.3",
    "fs-extra": "^9.1.0",
    "gibberish-detector": "^1.0.0",
    "is-reachable": "^5.0.0",
    "lorem-ipsum": "^2.0.3",
    "micromatch": "^4.0.2",
    "mongoose": "^5.11.15",
    "puppeteer": "^7.0.1",
    "puppeteer-extra": "^3.1.16",
    "puppeteer-extra-plugin-stealth": "^2.7.4",
    "random-email": "^1.0.3",
    "random-useragent": "^0.5.0",
    "random-words": "^1.1.1",
    "slice-ansi": "^4.0.0",
    "tree-kill": "^1.2.2",
    "uuid": "^8.3.2",
    "validator": "^13.5.2",
    "wrap-ansi": "^7.0.0"
  }
}